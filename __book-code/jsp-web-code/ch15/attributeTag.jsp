<%@ page contentType="text/html;charset=GB2312" %>
<%@ taglib uri="/mytag" prefix="my" %>
<html>
	<head>
		<title> 自定义attitudeTag</title>
	</head>
	<body>
		<h2>&lt;myfont&gt; 标签</h2>
		<my:myfont color="#009933">
  春眠不觉晓
</my:myfont>
		<br>
		<my:myfont color="#3366ff" border="3" bgColor="white">
  处处闻啼鸟
</my:myfont>
		<br>
		<my:myfont color="#99ffff" fontSize="5" bgColor="#C0C0C0">
  夜来风雨声
</my:myfont>
		<br>
		<my:myfont color="#ff00ff" border="7" bgColor="#aaaaaa" fontSize="6">
  花落知多少
</my:myfont>
	</body>
</html>

