<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- 开发环境 -->
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>
    <logger name="cn.timebusker" level="debug"/>
    <root level="info">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>
